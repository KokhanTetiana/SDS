{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["."],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [],
  "templates": {
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  }
}
